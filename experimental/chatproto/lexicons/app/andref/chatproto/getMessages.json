{
  "lexicon": 1,
  "id": "app.andref.chatproto.getMessages",
  "defs": {
    "main": {
      "type": "query",
      "description": "Get a list of the most recent messages on the network.",
      "parameters": {
        "type": "params",
        "required": ["channelNsid"],
        "properties": {
          "channelNsid": {
            "type": "string",
            "format": "nsid"
          },
          "before": {
            "type": "string",
            "format": "tid"
          },
          "hintChannelOwner": {
            "type": "string",
            "format": "at-identifier"
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": ["messages"],
          "properties": {
            "messages": {
              "type": "array",
              "items": {
                "type": "ref",
                "ref": "app.andref.chatproto.defs#messageView"
              }
            }
          }
        }
      }
    }
  }
}
